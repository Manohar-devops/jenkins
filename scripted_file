pipeline {
    agent {
        label 'java-agent-slave' && 'node-agenr-slave'
    }
    stages {
        stage ("Build") {
            steps {
                retry(3) {
                    echo "Welcome to Jenkins Pipeline"
                    error "Testing retry block using eror"
                }
                echo "**** After 3 retry's *******"
            }
            
        }
    }
}
