pipeline {
    agent {
        label 'java-agent-slave'
    }
    tools {
        maven 'MAVEN_PATH'
    }
    stages {
        stage ('Build') {
            steps {
                echo "Maven Version with default java version"
                sh 'mvn --version'
            }  
        }
        stage ('Build1') {
            tools {
                jdk 'jdk-17'
            }
            steps {
                echo "Maven version with specific java version"
                sh 'mvn --version'
            }
        }
    }
}
